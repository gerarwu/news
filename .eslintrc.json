{
    "parserOption": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "destructuring": true,
        "spread": true
      }
    },
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  }